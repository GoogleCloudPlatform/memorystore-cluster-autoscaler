                      "crossSeriesReducer": "REDUCE_MEAN",
                      "groupByFields": [],
                      "perSeriesAligner": "ALIGN_MEAN"
                    },
                    "filter": "metric.type=\"redis.googleapis.com/cluster/memory/size\" resource.type=\"redis.googleapis.com/Cluster\" resource.label.\"cluster_id\"=\"${memorystore_cluster_name}\" resource.label.\"location\"=\"${region}\""
                  }
                }
              },
              {
                "breakdowns": [],
